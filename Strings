#include <bits/stdc++.h>
using namespace std;

int main() {
	string a,b;
	getline(cin,a);
	getline(cin,b);
	printf("%d %d\n",a.size(),b.size());
	string s=a+b;
	cout << s << endl;
	swap(s[0],s[a.size()]);
	
	string s1=s.substr( 0,a.size() );
	string s2=s.substr(a.size(),s.size());
	
	cout << s1 << ' ' << s2 << endl;  
	
	return 0;
}
