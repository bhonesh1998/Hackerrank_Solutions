#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a,b,c,sum;
     cin>>a>>b>>c;
    if((a>=1&&a<=1000)||(b>=1&&b<=1000)||(c<=1000&&c>=1))
    {
       
        
        sum=a+b+c;
    }
    cout<<sum<<endl;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}

