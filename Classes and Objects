// Write your Student class here

class Student 
{
            int scores[5];
    int sum=0;
    public:
    void input ()
    {
            for(int i=0;i<5;i++)
            {
            int x;
            scanf("%d",&x);
            scores[i]=x;
            }
    }
    
    
    int calculateTotalScore()
    {
        for(int i=0;i<5;i++)
            {
           sum+=scores[i];
            
            }
        return sum;
    }
    
    
    
};
