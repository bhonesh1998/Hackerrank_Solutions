#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n,s=0,i;
    scanf("%d",&n);
    int arr[n];
     for(i=1;i<=n;i++)
         {scanf("%d",&arr[i]);
         }
   for(i=1;i<=n;i++)
       {s=s+arr[i];}
    printf("%d",s);
    return 0;
}

