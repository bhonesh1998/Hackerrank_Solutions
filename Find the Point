#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define z long long int
using namespace std ;
int main()
{
	
	int t;
	sf("%d",&t);
	while(t--)
	{
			int p1,p2,q1,q2;
			sf("%d %d %d %d",&p1,&p2,&q1,&q2);
			pf("%d %d\n",(2*q1-p1),(2*q2-p2));
	}

	return 0;
} 

