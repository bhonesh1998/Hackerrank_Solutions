#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    int a, b, c, d,max,i;
    scanf("%d %d %d %d", &a, &b, &c, &d);
     int z[]={a,b,c,d};
    max=z[0];
    for(i=0;i<=3;i++)
    {
        if(z[i]>max)
        {
            max=z[i];
        }
        
    }
    printf("%d",max);
    
    return 0;
}


