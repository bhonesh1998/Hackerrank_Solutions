#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std;



int main()
{

    int n;
    sf("%d",&n);
    vector <int> v;
    while(n--)
    {    int a ; sf("%d",&a); v.push_back(a);   
    } 
    int p;
    sf("%d",&p);
    v.erase(v.begin()+p-1);
    int s,e;
    sf("%d%d",&s,&e);
    v.erase(v.begin()+s-1,v.begin()+e-1);

    cout << v.size() << endl;
    for(int i=0;i<v.size();i++) pf("%d ",v[i]);

}
