#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define mod 1000000007
#define z long long int 
using namespace std;

int main()
{
		int n,h,c=0,i;

		sf("%d %d",&n,&h);
		int a[n];
		for(i=0;i<n;i++)
			sf("%d",&a[i]);
		sort(a,a+n);

			for(i=0;i<n;i++)
			{
				h=h-a[i];
				if(h>=0)
					c++;
				else
					break;

			}

		pf("%d",c);



return 0;

}
