#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int c=0,d=0;
    int a0; 
    int a1; 
    int a2; 
    scanf("%d %d %d",&a0,&a1,&a2);
    int b0; 
    int b1; 
    int b2; 
    scanf("%d %d %d",&b0,&b1,&b2);
    	if(a0>b0)
	{c++;}
	if(a0<b0)
	{d++;}
		if(a1>b1)
	{c++;}
		if(a1<b1)
	{d++;}
		if(a2>b2)
	{c++;}
		if(a2<b2)
	{d++;}
	
    printf("%d %d",c,d);
    return 0;
}

