#include <stdio.h>
#include <stdlib.h>

int main()
{   int n,v,i,f=0,j;
scanf("%d %d",&v,&n);
int a[n];
for(i=0;i<n;i++)
    scanf("%d",&a[i]);
for(i=0;i<n;i++)
for(j=0;j<n;j++)
{
    if(a[i]!=a[j])
        f=0;
}

  if(f==0&&(n>=1&&n<=1000)&&(v>=-1000&&v<=1000))
     {
for(i=0;i<n;i++)
{if(a[i]==v)
   printf("%d",i);}}
    return 0;
}

