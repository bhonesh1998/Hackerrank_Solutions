#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std ;
//unordered_set<int> s;
int main()
{
  
  int t;
  sf("%d",&t);
  while(t--)
  {
        int c=0,i;
        int h[26]={0};
        string s ; cin>>s;
        for(i=0;i<s.length();i++)
            h[s[i]-'a']++;
        for(i=0;i<26;i++)
        {
                if(h[i]>=1) c++; 
        }
        cout << c << endl;
  }



}
