#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define mod 1234567
#define z long long int 
using namespace std;

 
 
int main()
{
 
z t ;
sf("%lld",&t);
while(t--)
{
  z n ;
sf("%lld",&n);
n=n-1;
z p=1;
while(n--)
{
   z q;
   sf("%lld",&q);
   p = ((p%mod)*(q%mod))%mod;
}
  pf("%lld\n",p%mod);

}
 
    }
